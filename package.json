{
  "name": "jsonish",
  "version": "0.2.0",
  "license": "MIT",
  "homepage": "https://github.com/satoruk/jsonish-js",
  "keywords": [
    "typescript",
    "json",
    "convert",
    "transform"
  ],
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:package": "yarn build:prepackage && cd dest && yarn pack",
    "build:prepackage": "yarn test && yarn clean && yarn cp && yarn build",
    "ci": "yarn test --color",
    "clean": "rimraf dest",
    "commit": "git-cz",
    "cp": "cpx README.md dest",
    "deploy": "yarn build:prepackage && cd dest && yarn publish",
    "test": "jest",
    "test:inspect": "npx --node-arg --inspect-brk jest --runInBand --coverage=false"
  },
  "devDependencies": {
    "@types/jest": "25.2.1",
    "@types/lodash": ">=4.0.0",
    "commitizen": "4.0.5",
    "cpx": "1.5.0",
    "cz-conventional-changelog": "3.1.0",
    "jest": "25.5.3",
    "prettier": "2.0.5",
    "rimraf": "3.0.2",
    "semantic-release": "17.0.7",
    "ts-jest": "25.4.0",
    "ts-node": "8.9.1",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "lodash": ">=4.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
